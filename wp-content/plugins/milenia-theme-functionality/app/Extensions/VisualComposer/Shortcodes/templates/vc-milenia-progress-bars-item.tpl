<!-- - - - - - - - - - - - - - Progress Bar - - - - - - - - - - - - - - - - -->
<div class="milenia-progress-bars-item">
    <strong id="progress-bar-title-${progress_bar_id}" data-value="${progress_bar_value}" data-units="%" class="milenia-progress-bar-title">${progress_bar_title}</strong>
    <div role="progressbar" aria-valuenow="${progress_bar_value}" aria-valuemin="0" aria-valuemax="100" aria-labelledby="progress-bar-title-${progress_bar_id}" class="milenia-progress-bar">
        <div style="width: ${progress_bar_value}%" class="milenia-progress-bar-indicator"></div>
    </div>
</div>
<!-- - - - - - - - - - - - - - End of Progress Bar - - - - - - - - - - - - - - - - -->
